{
  "cli": {
    "version": ">= 16.4.1",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      }
    },
    "preview": {
      "distribution": "internal"
    },
    "preview2": {
      "environment": "production",
      "android": {
        "gradleCommand": ":app:assembleRelease",
        "buildType": "apk"
      }
    },
    "production": {
      "autoIncrement": true,
      "cache": {
        "key": "v1-cache",
        "paths": ["node_modules", "android/.gradle", "android/build", ".expo", ".tamagui"]
      }
    }
  },
  "submit": {
    "production": {}
  }
}
