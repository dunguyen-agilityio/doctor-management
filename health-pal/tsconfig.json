{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@theme/*": ["./src/theme/*"],
      "@theme": ["./src/theme"],
      "@utils-test": ["./src/utils-test"],
      "@app/*": ["./src/*"],
      "@/*": ["./*"]
    }
  },
  "include": ["**/*.ts", "**/*.tsx", ".expo/types/**/*.ts", "expo-env.d.ts"],
  "exclude": [".storybook/"]
}
