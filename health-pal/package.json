{
  "name": "health-pal",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "dev": "expo start --dev-client --reset-cache",
    "android": "expo run:android",
    "prebuild:a": "expo prebuild --clean -p android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "lint": "expo lint",
    "eas:dev": "eas build --platform android --profile development --local",
    "eas:prod": "eas build --platform android --profile preview2 --local",
    "test": "jest --coverage=true --changedSince=origin/feat/doctor-management",
    "test:w": "jest --watch --coverage=false --changedSince=origin/feat/doctor-management",
    "testDebug": "jest -o --watch --coverage=false",
    "testFinal": "jest",
    "updateSnapshots": "jest -u --coverage=false",
    "prepare": "cd .. && husky health-pal/.husky",
    "storybook-generate": "sb-rn-get-stories",
    "export": "expo export --source-maps --platform android --no-bytecode --no-minify -c",
    "analyze:android": "source-map-explorer 'dis/_expo/static/js/android/*.js' 'dist/_expo/static/js/android/*.js.map'"
  },
  "dependencies": {
    "@babel/plugin-transform-class-static-block": "^7.27.1",
    "@expo-google-fonts/inter": "^0.3.0",
    "@shopify/flash-list": "1.7.6",
    "@tamagui/babel-plugin": "^1.126.12",
    "@tamagui/config": "^1.126.12",
    "@tamagui/lucide-icons": "^1.126.12",
    "@tamagui/metro-plugin": "^1.126.13",
    "@tamagui/toast": "^1.126.12",
    "@tanstack/react-query": "^5.76.1",
    "babel-plugin-react-compiler": "^19.0.0-beta-af1b7da-20250417",
    "burnt": "^0.13.0",
    "dayjs": "^1.11.13",
    "expo": "~53.0.9",
    "expo-camera": "~16.1.6",
    "expo-dev-client": "~5.1.8",
    "expo-font": "~13.3.1",
    "expo-image": "~2.1.7",
    "expo-image-picker": "~16.1.4",
    "expo-linear-gradient": "^14.1.4",
    "expo-linking": "~7.1.5",
    "expo-router": "~5.0.6",
    "expo-secure-store": "~14.2.3",
    "expo-splash-screen": "~0.30.8",
    "expo-status-bar": "~2.2.3",
    "expo-system-ui": "^5.0.7",
    "react": "19.0.0",
    "react-content-loader": "^7.0.2",
    "react-hook-form": "^7.56.3",
    "react-native": "0.79.2",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-reanimated": "~3.17.4",
    "react-native-reanimated-carousel": "^4.0.2",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.10.0",
    "react-native-svg": "^15.12.0",
    "react-native-ui-datepicker": "^3.1.2",
    "tamagui": "^1.126.12",
    "zustand": "^5.0.5"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@gorhom/bottom-sheet": "^5.1.4",
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-native-community/cli": "^18.0.0",
    "@react-native-community/datetimepicker": "^8.3.0",
    "@react-native-community/slider": "^4.5.6",
    "@storybook/addon-ondevice-actions": "^8.6.2",
    "@storybook/addon-ondevice-controls": "^8.6.2",
    "@storybook/react-native": "^8.6.2",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.2.0",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.15.18",
    "@types/react": "~19.0.10",
    "babel-loader": "^8.4.1",
    "eslint": "^9.26.0",
    "eslint-config-expo": "~9.2.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.4.0",
    "expo-atlas": "0.4.0",
    "husky": "^9.1.7",
    "jest": "~29.7.0",
    "jest-expo": "~53.0.5",
    "lint-staged": "^16.0.0",
    "metro-minify-terser": "^0.82.4",
    "prettier": "^3.5.3",
    "react-dom": "^19.1.0",
    "source-map-explorer": "^2.5.3",
    "typescript": "~5.8.3"
  },
  "private": true,
  "jest": {
    "preset": "jest-expo",
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.ts",
      "@testing-library/jest-native/extend-expect"
    ],
    "setupFiles": [
      "<rootDir>/jest.setup.ts"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|tamagui|@tamagui|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@sentry/react-native|native-base|react-native-svg)"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/**/*.test.{ts,tsx}",
      "!src/**/mocks/**",
      "!src/**/types/**",
      "!src/**/theme/**",
      "!src/**/models/**",
      "!src/**/icons/**"
    ]
  },
  "eslintConfig": {
    "extends": "universe/native"
  }
}
