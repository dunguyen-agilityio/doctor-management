{
  "semi": true,
  "singleQuote": true,
  "printWidth": 80,
  "tabWidth": 2,
  "useTabs": false,
  "endOfLine": "auto",
  "arrowParens": "always",
  "importOrder": [
    "^react",
    "^react-native",
    "^@react-navigation/(.*)$",
    "^expo-font",
    "<THIRD_PARTY_MODULES>",
    "^@/navigation",
    "^@/screens",
    "^@/components",
    "^@/constants",
    "^@/types",
    "^@/services",
    "^@/utils",
    "^@/hooks",
    "^@/mocks",
    "^[./]"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
  "importOrderBuiltinModulesToTop": true,
  "plugins": ["@trivago/prettier-plugin-sort-imports"]
}
