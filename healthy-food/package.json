{
  "name": "healthy-food-app",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "storybook": "STORYBOOK_ENABLED=true expo start --dev-client",
    "dev": "expo start --dev-client --clear",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "lint": "eslint *.ts{,x}",
    "lint:fix": "eslint --fix ./src/**/*.ts{,x}",
    "format": "prettier --write .",
    "testDebug": "jest -o --watch --coverage=false",
    "test": "jest",
    "updateSnapshots": "jest -u --coverage=false",
    "storybook-generate": "sb-rn-get-stories",
    "build-debug": "cd android && ./gradlew assembleDebug",
    "build-release": "cd android && ./gradlew assembleRelease",
    "clean": "cd android && ./gradlew clean",
    "esa-build-android": "eas build --profile development --platform android",
    "prebuild": "expo prebuild --clean --platform android",
    "prepare": "cd .. && husky healthy-food/.husky",
    "analyze:android": "source-map-explorer 'dist/_expo/static/js/android/*.js' 'dist/_expo/static/js/android/*.js.map'",
    "eas-build-pre-install": "./scripts/create-google-services.sh"
  },
  "jest": {
    "preset": "jest-expo",
    "setupFiles": [
      "./jest.setup.js"
    ],
    "moduleNameMapper": {
      "^src/(.*)$": "<rootDir>/src/$1"
    }
  },
  "lint-staged": {
    "src/**/*.{js,ts,tsx}": [
      "prettier --write",
      "eslint"
    ]
  },
  "dependencies": {
    "@expo/metro-config": "^0.19.11",
    "@expo/metro-runtime": "~4.0.1",
    "@notifee/react-native": "^9.1.8",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-native-firebase/app": "^21.12.0",
    "@react-native-firebase/messaging": "^21.12.0",
    "@react-native/metro-config": "^0.77.1",
    "@react-navigation/bottom-tabs": "^7.2.0",
    "@react-navigation/elements": "^2.2.5",
    "@react-navigation/native": "^7.0.14",
    "@react-navigation/native-stack": "^7.2.0",
    "@tanstack/react-query": "^5.66.0",
    "babel-plugin-react-compiler": "^19.0.0-beta-bafa41b-20250307",
    "expo": "~52.0.38",
    "expo-build-properties": "~0.13.2",
    "expo-constants": "~17.0.5",
    "expo-font": "~13.0.3",
    "expo-image": "~2.0.6",
    "expo-linear-gradient": "~14.0.2",
    "expo-linking": "~7.0.5",
    "expo-splash-screen": "~0.29.21",
    "expo-status-bar": "~2.0.1",
    "react": "18.3.1",
    "react-compiler-runtime": "^19.0.0-beta-bafa41b-20250307",
    "react-native": "0.76.7",
    "react-native-gesture-handler": "^2.24.0",
    "react-native-reanimated": "~3.16.1",
    "react-native-reanimated-carousel": "^4.0.2",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "~4.4.0",
    "react-native-shimmer-placeholder": "^2.0.9",
    "react-native-toast-message": "^2.2.1",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.26.9",
    "@babel/plugin-transform-class-properties": "^7.25.9",
    "@babel/preset-react": "^7.26.3",
    "@eslint/compat": "^1.2.6",
    "@gorhom/bottom-sheet": "^5.1.1",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-native-community/cli": "^15.1.3",
    "@react-native-community/datetimepicker": "8.2.0",
    "@react-native-community/slider": "4.5.5",
    "@storybook/addon-ondevice-actions": "^8.5.4",
    "@storybook/addon-ondevice-controls": "^8.5.4",
    "@storybook/react": "^8.5.5",
    "@storybook/react-native": "^8.5.4",
    "@tanstack/eslint-plugin-query": "^5.66.0",
    "@testing-library/react-native": "^12.9.0",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@tsconfig/react-native": "^2.0.3",
    "@types/jest": "^29.5.14",
    "@types/react": "^18.0.28",
    "@types/react-native": "~0.70.6",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^8.24.1",
    "@typescript-eslint/parser": "^8.24.1",
    "babel-loader": "^8.3.0",
    "babel-plugin-module-resolver": "^5.0.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "eslint": "^9.20.1",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-compiler": "^19.0.0-beta-bafa41b-20250307",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-native": "^5.0.0",
    "eslint-plugin-testing-library": "^7.1.1",
    "expo-atlas": "^0.4.0",
    "expo-dev-client": "^5.0.15",
    "expo-dev-menu": "^6.0.19",
    "globals": "^15.15.0",
    "husky": "^9.1.7",
    "jest": "~29.7.0",
    "jest-expo": "~52.0.6",
    "lint-staged": "^15.4.3",
    "prettier": "^3.5.0",
    "prettier-eslint": "^16.3.0",
    "react-native-svg": "15.8.0",
    "react-test-renderer": "^18.2.0",
    "source-map-explorer": "^2.5.3",
    "typescript": "^5.3.3"
  },
  "private": true
}
